# Example environment variables for connecting to your Sanity project.
# Copy this file to `.env` and replace the placeholders with your actual values.

# Public variables are exposed to the client. Use the prefix PUBLIC_ for Astro.
PUBLIC_SANITY_PROJECT_ID=your_project_id
PUBLIC_SANITY_DATASET=production
PUBLIC_SANITY_API_VERSION=2023-05-03

# Private token for draft/previews (optional). Do not expose this on the client.
SANITY_API_READ_TOKEN=your_sanity_read_token
